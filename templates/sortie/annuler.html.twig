{% extends 'base.html.twig' %}

{% block body %}
    {% for label, messages in app.flashes %}
        <div class="alert alert-{{ label }}">
            {% for message in messages %}
                <p>{{ message }}</p>
            {% endfor %}
        </div>
    {% endfor %}

    <h1>Annuler la sortie {{ sortie.nom }}</h1>
    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}
    <div>
        <a class="btn btn-secondary mt-2" href="{{ path('sortie_liste') }}">Retour Ã  la liste</a>
    </div>
    {{ form_end(form) }}

{% endblock %}

{% block title %}
    Annuler | {{ parent() }}
{% endblock %}

