{% extends 'base.html.twig' %}


{% block body %}
<h1>Mon profil</h1>
    <form class="col g-3">
        <div class="col-md-4">
            <label for="inputState" class="form-label">Campus</label>
            <select id="inputState" class="form-select">
                <option selected>Choose...</option>
                <option>...</option>
            </select>
        </div>
        <div class="col-md-4">
            <label for="inputTelephone" class="form-label">Pseudo</label>
            <input type="text" class="form-control" id="inputTelephone">
        </div>
        <div class="col-md-4">
            <label for="inputTelephone" class="form-label">Prénom</label>
            <input type="text" class="form-control" id="inputTelephone">
        </div>
        <div class="col-md-4">
            <label for="inputTelephone" class="form-label">Nom</label>
            <input type="text" class="form-control" id="inputTelephone">
        </div>
        <div class="col-md-4">
            <label for="inputTelephone" class="form-label">Téléphone</label>
            <input type="text" class="form-control" id="inputTelephone">
